{
  "rewrites": [
    { "source": "/worker/:path*", "destination": "/index.html" },
    { "source": "/admin/:path*", "destination": "/index.html" },
    { "source": "/:path*", "destination": "/index.html" }
  ],
  "builds": [
    { "src": "dist/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
